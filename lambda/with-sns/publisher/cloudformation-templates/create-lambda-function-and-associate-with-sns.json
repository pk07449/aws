{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "lambda Enablement service",
  "Resources": {
    "sales-orders-sns-function": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "Code": {
          "S3Bucket": "pankaj-lambda-functions-bucket",
          "S3Key": "lambda.methodhandler-1.0.2.jar"
        },
        "FunctionName": "sales-order-sns-function",
        "Handler": "com.pankaj.aws.java.lambda.sns.requestMethodHandler.RequestHandlerLambda::handleRequest",
        "MemorySize": 128,
        "Role": "arn:aws:iam::292661884468:role/handleRequestMethodLambdaRole",
        "Runtime": "java8"
      }
    }
  }
}